<%- include('../partials/header') %>

<div class="container">
	<div class="row">
		<% for (let product of products) { %>
			<div class="col-md-8">
				<a href="/products/<%= product.id %>"><h1><%= product.title %></h1></a>
				<h6><strong>$ <%= product.price %></strong></h6>
				<p><%= product.description %></p>
			</div>
		<% } %>
	</div>
</div>

<%- include('../partials/footer') %>